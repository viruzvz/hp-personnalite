.col-md-12
  .card
    mixin card(data, src, src2, name, prof, prof1, prof2, prof3, prof4, prof5)
      .card-box.card-hover
        .Embed.Embed--3by2
          .triangle.triangle-0.triangle-absolute
            .text-more
              a(class='btn-link' data-toggle="modal" data-target=data).t-color--one veja mais
          .circle-absolut
            img.media-object.img-circle.img-thumbnail.add-shadow.img-card(src=src)
          a.pointer(class='btn-link' data-toggle="modal" data-target=data)
            img.img-responsive(src=src2)  
        .card-caption
          p:strong.fs--four
            = name
          ul.list-unstyled
            if prof
              li
                i.fa.fa-graduation-cap 
                i  
                  = prof
            if prof1
              li
                i.fa.fa-graduation-cap 
                i
                  = prof1
            if prof2
              li
                i.fa.fa-graduation-cap 
                i  
                  = prof2
            if prof3
              li
                i.fa.fa-graduation-cap 
                i  
                  = prof3
            if prof4
              li
                i.fa.fa-graduation-cap 
                i  
                  = prof4
            if prof5
              li
                i.fa.fa-graduation-cap 
                i  
                  = prof5

    +card('.modal-thales','assets/img/equipe2018/DrThales.jpg', 'assets/img/album2018/1.jpg', 'Dr. Thales Brasil', 'Doutorado em Implantodontia.', 'Mestrado em Implantodontia.', 'Especialista em Implantodontia.', 'Especialista em Periodontia.', 'Especialista em Cirurgia Oral menor.', 'Consultor Científico em Biomateriais.', 'Diretor clinico Instituto Personnalité.')
    +card('.modal-ivanir','assets/img/equipe2018/DrIvanir.jpg', 'assets/img/album2018/63.jpg', 'Dr. Ivanir Grecco', 'Mestrado em Cirurgia Buco-Maxilo-Facial', 'Especialista em Cirurgia Buco-Maxilo-Facial.', 'Cirurgião Buco-Maxilo-Facial do Hospital Regional da Ceilândia.', 'Professor de Cirurgia Buco-Maxilo-Facial da Unb.', '', '', '')
    +card('.modal-jalusa','assets/img/equipe2018/DrJalusa.png', 'assets/img/album2018/64.jpg', 'Dra. Jalusa Tassinari', 'Especialista em Ortodontia.', 'Especialista em Saúde Pública.', '', '', '', '', '')
    +card('.modal-thayse','assets/img/equipe2018/DrThayse.jpg', 'assets/img/album2018/65.jpg', 'Dra. Thaise Cristina', 'Especialista em Prótese.', 'Especialista em Odontologia Hospitalar Intensiva.', '', '', '', '', '')
    +card('.modal-rogerio','assets/img/equipe2018/DrRogerio.jpg', 'assets/img/album2018/45.jpg', 'Dr. Rogerio Duarte', 'Especialista em Endodontia.', '', '', '', '', '', '')
    +card('.modal-maira','assets/img/equipe2018/DrMaira.jpg', 'assets/img/album2018/6.jpg', 'Dra. Maíra Mascarenhas', 'Especialização em Endodontia.', '', '', '', '', '', '')
    +card('.modal-joao','assets/img/equipe2018/DrJoao.jpg', 'assets/img/album2018/29.jpg', 'Dr. João Henrique Mascarenhas', 'Especialista em Prótese.', '', '', '', '', '', '')
    +card('.modal-larissa','assets/img/equipe2018/DrLarissa.jpg', 'assets/img/album2018/66.jpg', 'Dra. Larissa Freire', 'Especialização em DTM e Dor Orofacial.', 'Pós-Graduada em Harmonização Orofacial.', '', '', '', '', '')
    +card('.modal-mayla','assets/img/equipe2018/DrMayla.jpg', 'assets/img/album2018/52.jpg', 'Dra. Mayla Ditzel', 'Especialização em Endodontia.', '', '', '', '', '', '')
    +card('.modal-carolina','assets/img/equipe2018/DrCarolina.jpg', 'assets/img/album2018/51.jpg', 'Dra. Carolina Toledo', 'Especialização em Odontopediatria.', '', '', '', '', '', '')


include modal/thales
include modal/ivanir
include modal/jalusa
include modal/thayse
include modal/rogerio
include modal/maira
include modal/joao
include modal/larissa
